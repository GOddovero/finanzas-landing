<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f8f9fa"/>
  <rect x="50" y="50" width="300" height="200" rx="10" fill="#0a2647" fill-opacity="0.1"/>
  
  <!-- Gráfico de línea de inversión -->
  <polyline points="80,200 120,180 160,190 200,150 240,120 280,90 320,110" 
            fill="none" stroke="#205295" stroke-width="3"/>
  
  <!-- Área bajo la curva -->
  <path d="M 80,200 L 120,180 L 160,190 L 200,150 L 240,120 L 280,90 L 320,110 L 320,200 Z" 
        fill="#205295" fill-opacity="0.2"/>
  
  <!-- Puntos de datos -->
  <circle cx="80" cy="200" r="5" fill="#0a2647"/>
  <circle cx="120" cy="180" r="5" fill="#0a2647"/>
  <circle cx="160" cy="190" r="5" fill="#0a2647"/>
  <circle cx="200" cy="150" r="5" fill="#0a2647"/>
  <circle cx="240" cy="120" r="5" fill="#0a2647"/>
  <circle cx="280" cy="90" r="5" fill="#0a2647"/>
  <circle cx="320" cy="110" r="5" fill="#0a2647"/>
  
  <!-- Iconos de moneda -->
  <text x="100" y="90" font-family="Arial" font-size="14" font-weight="bold" fill="#0a2647">$</text>
  <text x="150" y="120" font-family="Arial" font-size="14" font-weight="bold" fill="#0a2647">€</text>
  <text x="220" y="80" font-family="Arial" font-size="14" font-weight="bold" fill="#0a2647">£</text>
  <text x="290" y="70" font-family="Arial" font-size="14" font-weight="bold" fill="#0a2647">¥</text>
  
  <!-- Título del proyecto -->
  <text x="200" y="250" font-family="Arial" font-size="16" font-weight="bold" fill="#0a2647" text-anchor="middle">Estrategia Patrimonial</text>
</svg>
